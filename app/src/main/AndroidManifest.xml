<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swayam.mobilestore">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.swayam.mobilestore.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EComerceApp">
        <activity android:name="com.swayam.mobilestore.CartActivity"></activity>
        <activity android:name="com.swayam.mobilestore.ProductDetailsActivity" />
        <activity android:name="com.swayam.mobilestore.ProductList" />
        <activity android:name="com.swayam.mobilestore.HomeActivity" />
        <activity android:name="com.swayam.mobilestore.RegisterActivity" />
        <activity android:name="com.swayam.mobilestore.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>